        </activity>
        <activity android:name=".resultscreenactivities.ResultScreenFirst" />
        <activity android:name=".calcactivities.CalcActivity" />
        <activity android:name=".loginactivities.HomeScreen" />
        <activity android:name=".loginactivities.aboutactivity" />
